server {
   listen 80;
   # If necessary, set server name
   # server_name <%= node.ipynb.proxy.hostname %> <%= node.ipynb.proxy.alias_hostnames.join(" ") %>;

   access_log off;
   error_log /var/log/ .......

   location / {
      proxy_pass http://localhost:<%= node[:ipynb][:NotebookApp][:port] %>;
      proxy_set_header Host $host;
      proxy_buffering off;
   }
}
